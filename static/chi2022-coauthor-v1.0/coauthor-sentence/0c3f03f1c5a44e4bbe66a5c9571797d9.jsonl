[
    {
        "text": "An alien has kidnapped Matt Damon, not knowing what lengths humanity goes through to retrieve him whenever he goes missing.",
        "source": "api",
        "start_time": 0.0,
        "end_time": 0.0,
        "start_progress": 0.0,
        "end_progress": 0.082044384667115
    },
    {
        "text": "The aliens have no idea what theyâ€™re in for.",
        "source": "user",
